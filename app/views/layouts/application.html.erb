<!DOCTYPE html>
<html>
  <head>
    <title>Oldcoins</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
    <ul class="nav navbar-nav">
      <li> <%= link_to t("dynasties.all"), root_path %></li>
    </ul>	
    </div>
    <ul class="nav navbar-nav">
      <li><a href="/dynasties/new"> <%= t"dynasties.new" %> </a></li>
      <li><a href="/newfounds"> <%= t"dynasties.newfound" %> </a></li>
    </ul>
    <ul class="nav navbar-nav navbar-right">
      <ul>
      <li><%= link_to_unless_current "Deutsch",locale: "de" %></li>
      <li><%= link_to_unless_current "العربيه",locale: "ar" %></li>
      </ul>
    </ul>
  </div>
</nav>
    <%= yield %>
  </body>
</html>
